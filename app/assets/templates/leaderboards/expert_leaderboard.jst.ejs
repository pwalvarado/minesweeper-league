<table class="table table-condensed">
  <caption>Expert</caption>
  <thead>
    <tr>
      <th class="col-md-3">Rank</th>
      <th class="col-md-6">Name</th>
      <th class="col-md-3">Time</th>
    </tr>
  </thead>
  <tbody>
    <% var rank = 1 %>
    <% expertLeaders.each(function (expertLeader) { %>
      <tr>
        <th scope="row"><%= rank %></th>
        <td><%= expertLeader.get('name') %></td>
        <td><%= expertLeader.get('time') %></td>
      </tr>
      <% rank += 1 %>
    <% }) %>
  </tbody>
</table>
